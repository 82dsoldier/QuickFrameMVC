@model MimeTypeDto
@{ 
	ViewBag.Title = "File Extension";
	Layout = "_PopupLayout";
}
<form qf-form>
    <div asp-validation-summary="All" class="text-danger"></div>
	<div class="container">
		<div class="col-md-6">
			<label asp-for="Name"></label>
			<div>
				<input asp-for="Name" class="form-control" />
			</div>
		</div>
		<div class="col-md-12">
			<label asp-for="Description"></label>
			<div>
				<textarea asp-for="Description" class="form-control"></textarea>
			</div>
		</div>
		<div class="col-md-6">
			<label asp-for="MimeTypeIdentifier"></label>
			<div>
				<input asp-for="MimeTypeIdentifier" class="form-control" />
			</div>
		</div>
		@Html.Partial("FormButtons");
	</div>
</form>

@section scripts {

}
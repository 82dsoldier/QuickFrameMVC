@model QuickFrame.Mvc.Interfaces.ITreeModel

	<ul>
		@{ 
			if(Model.Nodes != null && Model.Nodes.Any()) {
				foreach(var node in Model.Nodes) {
					@await Component.InvokeAsync(nameof(QuickFrame.Mvc.ViewComponents.TreeNodeComponent), new { model = node });
				}
			}
		}
		@*@{ 
			if(Model?.Nodes != null && Model.Nodes.Any()) {
				foreach(var node in Model.Nodes) {
					await Component.InvokeAsync(nameof(QuickFrame.Mvc.ViewComponents.TreeNodeComponent), node);
				}
			}
		}*@
	</ul>
